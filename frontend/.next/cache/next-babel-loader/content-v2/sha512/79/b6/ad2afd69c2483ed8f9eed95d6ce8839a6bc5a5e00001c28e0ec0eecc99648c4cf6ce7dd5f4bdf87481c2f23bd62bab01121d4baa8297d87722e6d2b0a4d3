{"ast":null,"code":"var __jsx = React.createElement;\nimport React from \"react\";\nimport IconButton from \"@material-ui/core/IconButton\";\nimport { useRouter } from \"next/router\";\nimport NavigateBeforeIcon from \"@material-ui/icons/NavigateBefore\";\nimport NavigateNextIcon from \"@material-ui/icons/NavigateNext\";\nconst NUM_OF_NAV_PAGES = 3;\n\nconst PageNav = ({\n  pageNumber,\n  numOfPages,\n  curDir = \"\"\n}) => {\n  const router = useRouter();\n  let navigablePages = [];\n  let start = NUM_OF_NAV_PAGES * (Math.ceil(pageNumber / NUM_OF_NAV_PAGES) - 1) + 1;\n  let end = Math.min(NUM_OF_NAV_PAGES * Math.ceil(pageNumber / NUM_OF_NAV_PAGES), numOfPages);\n\n  if (pageNumber !== 1) {\n    navigablePages.push(__jsx(IconButton, {\n      onClick: () => {\n        router.push(`${curDir}/posts/pages/${pageNumber - 1}`);\n      }\n    }, __jsx(NavigateBeforeIcon, null)));\n  }\n\n  if (start !== 1) {\n    navigablePages.push(__jsx(IconButton, {\n      onClick: () => {\n        router.push(`${curDir}/posts/pages/${start - 1}`);\n      }\n    }, \"...\"));\n  }\n\n  for (let i = start; i <= end; i++) {\n    if (pageNumber === i) {\n      navigablePages.push(__jsx(IconButton, {\n        disabled: true,\n        onClick: () => {\n          router.push(`${curDir}/posts/pages/${i}`);\n        }\n      }, i));\n    } else {\n      navigablePages.push(__jsx(IconButton, {\n        onClick: () => {\n          router.push(`${curDir}/posts/pages/${i}`);\n        }\n      }, i));\n    }\n  }\n\n  if (end !== numOfPages) {\n    navigablePages.push(__jsx(IconButton, {\n      onClick: () => {\n        router.push(`${curDir}/posts/pages/${end + 1}`);\n      }\n    }, \"...\"));\n  }\n\n  if (pageNumber !== numOfPages) {\n    navigablePages.push(__jsx(IconButton, {\n      onClick: () => {\n        router.push(`${curDir}/posts/pages/${pageNumber + 1}`);\n      }\n    }, __jsx(NavigateNextIcon, null)));\n  }\n\n  return __jsx(\"nav\", null, navigablePages);\n};\n\nexport default PageNav;","map":null,"metadata":{},"sourceType":"module"}